<AppNavbar />
<AppToolbar @title="My Network"/>
<AppContainer>
  <NavbarTabs
    @tabs={{array
      (hash
        displayName = "Feed"
        route = "my-network.feed"
        icon="rss"
      )
      (hash
        displayName = "Search"
        route = "my-network.search"
        icon="search"
      )
      (hash
        displayName = "Friends"
        route = "my-network.friends"
        icon="users"
      )
      (hash
        displayName = "Friend Requests"
        route = "my-network.friend-requests"
        icon="exchange-alt"
      )
    }}
  />
  <PageContainer>
    {{outlet}}
  </PageContainer>
</AppContainer>
